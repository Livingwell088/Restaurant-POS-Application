<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchorPane" prefHeight="600.0" prefWidth="900.0" stylesheets="@../../resources/styles/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project.controllers.logoPageController">
   <children>
      <AnchorPane fx:id="content" prefHeight="200.0" prefWidth="200.0">
         <children>
            <VBox fx:id="vbox" alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Label fx:id="name" alignment="CENTER" style="-fx-font-size: 100;" text="Ming House" />
                  <Button fx:id="button" mnemonicParsing="false" onAction="#toMainPage">
                     <graphic>
                        <ImageView fx:id="image" fitHeight="350.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/building.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <HBox prefHeight="100.0" prefWidth="200.0" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
